<Project Sdk="Microsoft.NET.Sdk">
<PropertyGroup>
	  <TargetFramework>net48</TargetFramework>
	  <Nullable>enable</Nullable>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <LangVersion>latest</LangVersion>
	  <PlatformTarget>x64</PlatformTarget>
	  <Platforms>x64</Platforms>
	  <UseWPF>true</UseWPF>
	  <BaseOutputPath>$(SolutionDir)build\bin\</BaseOutputPath>
	  <BaseIntermediateOutputPath>$(SolutionDir)build\obj\$(MSBuildProjectName)</BaseIntermediateOutputPath>
	  <UseCommonOutputDirectory>true</UseCommonOutputDirectory>
	  <AutoGenerateBindingRedirects>False</AutoGenerateBindingRedirects>
	  <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	  <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <PackageId>SAAC-$(AssemblyName)</PackageId>
	  <version>1.0.0-pre</version>
  </PropertyGroup>
<ItemGroup>
  <PackageReference Include="DocumentFormat.OpenXml" Version="3.3.0" />
</ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Whisper\Whisper.csproj" />
  </ItemGroup>

</Project>
